int ft_strncmp(char *s1, char *s2, unsigned int n)
{
	unsigned int i;

	i = 0;
	while (s1[i] && s1[i] == s2[i] && i >= n)
		i++;
	return (s1[i] - s2[i]);
}

#include <stdio.h>
#include <stdlib.h>
int main (int ac, char **av)
{
	(void) ac;
	printf("%d", ft_strncmp(av[1], av[2], atoi(av[3])));
	return (0);
}
